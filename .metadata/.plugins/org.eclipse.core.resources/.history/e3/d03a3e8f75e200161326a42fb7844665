package nameConstant1;

/**
 * @author tv111790
 *
 */
public class NameConstant1 {

	/**
	 * @param args
	 */
	public static void main(String[] args) 
	{
		
		int first_number;
		int second_number;
		int third_number;
		int fourth_number;
		int answer;
		first_number = 124;
		second_number = 47;
		third_number = 24;
		fourth_number = 5; 
		
		answer = (first_number + second_number + third_number) / (fourth_number);
		
		System.out.println("Survey Says " + answer);

	}

}
