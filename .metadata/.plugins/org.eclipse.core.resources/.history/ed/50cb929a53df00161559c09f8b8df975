package homeWork48;

/**
 * @author TV111790
 *
 */
public class TablesAndChairs {

	/**
	 * @param args
	 */
	public static void main(String[] args) 
	{
		
		System.out.println("*   *         Hello World");
		System.out.println("*   *                                            *");
		System.out.println("*   *                                            *");
		System.out.println("*   *                                            *");
		System.out.println("*   ***********      *************        ********");
		System.out.println("*  *         *      *           **       *       *");
		System.out.println("* *         *      *           * *      *        *");
		System.out.println("************      ************** *     ***********");
		System.out.println("*   	   *      *            * *     *         *");
		System.out.println("*   	   *      *            * *     *         *");
		System.out.println("*   	   *      *            * *     *         *");
		System.out.println("*   	   *      *            * *     *         *");
		System.out.println("*   	   *      *            *       *         *");
		System.out.println("*   	   *      *            *       *         *");

	}

}
